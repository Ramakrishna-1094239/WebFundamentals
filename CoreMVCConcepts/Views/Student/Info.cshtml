
@{
    ViewData["Title"] = "Info";
}

<h1>@TempData["Message"]</h1>
<br />

<p> There are @TempData["StudentCount"] have joined</p>

<table class="table">
    <thead>
        <tr>
            <th>Student ID</th>
            <th>Name</th>
            <th>City></th>
            <th>Gender</th>

        </tr>

    </thead>
    <tbody>
        @foreach (var item in TempData["StudentList"] as IList<Student>)
        {
            <tr>
                <td>@item.StudentID</td>
                <td>@item.Name</td>
                <td>@item.City</td>
                <td>@item.Gender</td>
            </tr>

        }


    </tbody>

</table>